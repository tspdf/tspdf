{
  "name": "@tspdf/pdf-core",
  "version": "v0.0.0",
  "description": "Core package to wrap up PDF.js and provide a consistent API for PDF rendering.",
  "author": "TS PDF",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0"
  },
  "peerDependencies": {
    "pdfjs-dist": "^5.2.133"
  },
  "devDependencies": {
    "@tspdf/eslint-config": "*",
    "@tspdf/rollup-config": "*",
    "@tspdf/typescript-config": "*",
    "eslint": "^9.28.0",
    "pdfjs-dist": "^5.2.133",
    "rollup": "^4.40.2",
    "typescript": "^5.8.3"
  }
}
