{
  "name": "@tspdf/pdf-core",
  "version": "v0.0.0",
  "description": "Core package to wrap up PDF.js and provide a consistent API for PDF rendering.",
  "author": "TS PDF",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.esm.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config && npm run copy-license",
    "copy-license": "cp ../../licenses/pdfjs-dist.txt dist/pdfjs-dist/LICENSE-pdfjs-dist.txt",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "pdfjs-dist": "^5.2.133"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@tspdf/eslint-config": "*",
    "@tspdf/typescript-config": "*",
    "esbuild": "^0.25.0",
    "eslint": "^9.28.0",
    "rollup": "^4.24.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.2.0",
    "typescript": "^5.8.3"
  }
}
