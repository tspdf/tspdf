{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "ES2015"
    ],
    "module": "ESNext",
    "target": "es6",
    "outDir": "dist",
    "declaration": true,
    "moduleResolution": "node",
    "types": [
      "react",
      "react-dom"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}